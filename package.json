{
    "name": "speed_limit_backend",
    "type": "module",
    "version": "1.0.0",
    "description": "speed_limit_backend is the ultimate backend solution",
    "main": "src/server.js",
    "scripts": {
        "dev": "node --import ./register.js src/server.js --watch",
        "setup": "bun install && bun update",
        "start": "node --import ./register.js src/server.js"
    },
    "keywords": [
        "express",
        "backend",
        "speed_limit_backend"
    ],
    "license": "UNLICENSED",
    "dependencies": {
        "@prisma/adapter-pg": "^7.0.1",
        "@prisma/client": "7",
        "@types/cors": "^2.8.19",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^4.21.2",
        "express-custom-error": "^1.9.4",
        "has-keys": "^0.1.0",
        "helmet": "^3.23.3",
        "http-status": "^1.8.1",
        "i": "^0.3.7",
        "mandatoryenv": "^1.1.6",
        "module-alias": "^2.2.3",
        "morgan": "^1.10.1",
        "nodemon": "^3.1.11",
        "pg": "^8.16.3",
        "rotating-file-stream": "^1.4.6"
    },
    "devDependencies": {
        "prisma": "7"
    },
    "_moduleAliases": {
        "@root": "src",
        "@controllers": "src/controllers",
        "@models": "src/models",
        "@util": "src/util",
        "@routes": "src/routes"
    },
    "imports": {
        "#controllers/*": "./src/controllers/*",
        "#models/*": "./src/models/*",
        "#util/*": "./src/util/*",
        "#routes/*": "./src/routes/*",
        "#*": "./src/*"
    },
    "dependencies_note": "tsx is used for @ alias support"
}
